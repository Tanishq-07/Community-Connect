<% layout('layouts/boilerplate') %>
    <main>
        <h1>Create New Community</h1>
        <form method="POST" action="/community/new">
            <label for="name">Community Name</label>
            <input type="text" name="name" id="name" alt="Community Name" required>
            <label for="description">Description</label>
            <input type="text" name="description" id="description" alt="Community Description" required>
            <button>Create</button>
        </form>
    </main>
